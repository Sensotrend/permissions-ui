{
  "name": "permissions-ui",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "file-saver": "^2.0.5",
    "jose": "^4.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "del-cli webapp/* !webapp/classes !webapp/WEB-INF !webapp/META-INF",
    "build": "react-scripts build",
    "postbuild": "npm run clean && npm run copy-built",
    "copy-built": "copyfiles -u 1 -a \"build/**/*.*\" webapp/",
    "lint": "./node_modules/eslint/bin/eslint.js src/",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://dev.sensotrend.fi/permissions-ui"
}
